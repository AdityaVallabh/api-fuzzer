{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "mqgen",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}/mqgo/bin/mqgen.exe",
            "env": {},
            "args": ["-meqa", "d:\\src\\autoapi\\mqgo\\meqa_data"],
            "showLog": true
        },
        {
            "name": "mqtag",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}/mqgo/bin/mqtag.exe",
            "env": {},
            "args": ["-meqa", "d:\\src\\autoapi\\mqgo\\meqa_data", "-y"],
            "showLog": true
        },
    {
        "name": "Python",
        "type": "python",
        "request": "launch",
        "stopOnEntry": true,
        "pythonPath": "${config:python.pythonPath}",
        "program": "${file}",
        "cwd": "${workspaceRoot}/mqserver",
        "env": {},
        "envFile": "${workspaceRoot}/.env",
        "debugOptions": [
            "WaitOnAbnormalExit",
            "WaitOnNormalExit",
            "RedirectOutput"
        ]
    },
        {
            "name": "mqgo",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}/mqgo/bin/mqgo.exe",
            "env": {},
            "args": ["-meqa", "d:\\src\\autoapi\\mqgo\\meqa_data"],
            "showLog": true
        }
    ]
}